@model IEnumerable<EntitiesViewModel.TeamLeadViewModel>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Areas/Admin/Views/Shared/_Layout.cshtml";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.TeamLeadId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Name)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.TeamLeadId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
                <td>
                    <a href="@Url.Action("Edit", "TeamLead")?id=@item.TeamLeadId">Edit</a> |
                    <a href="@Url.Action("Details", "TeamLead")?id=@item.TeamLeadId">Details</a> |
                    <a href="@Url.Action("Delete", "TeamLead")?id=@item.TeamLeadId">Delete</a>
                </td>


        </tr>
}
    </tbody>
</table>
